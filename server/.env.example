# Copy this to .env and fill in your values

# Supabase PostgreSQL connection string
# Found at: Supabase Dashboard → Settings → Database → Connection string (URI mode)
DATABASE_URL=postgres://postgres:[YOUR-PASSWORD]@[YOUR-PROJECT].supabase.co:5432/postgres

# Session secret — generate a random string (e.g. openssl rand -hex 32)
SESSION_SECRET=change-me-to-a-long-random-string

# CORS: where your frontend is served from
# Local dev: http://localhost:5173
# Production: your actual frontend URL
ALLOWED_ORIGIN=http://localhost:5173
